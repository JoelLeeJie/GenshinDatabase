<Window x:Class="GenshinDB_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GenshinDB_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1200">
    <Grid Margin="0,0,0,-16">
        <TextBox HorizontalAlignment="Right" Height="33" Margin="0,17,1056,0" TextWrapping="Wrap" Text="GenshinDB" VerticalAlignment="Top" Width="125" FontSize="20" TextAlignment="Center"/>
        <Frame x:Name="PageFrame" Margin="0,55,10,10" NavigationUIVisibility="Hidden"/>
        <Button x:Name="Character" Content="Character" Margin="176,10,819,664" Background="#FFD6E8FF" RenderTransformOrigin="0.5,0.5" Click="Character_Click"/>
        <Button x:Name="Weapon" Content="Weapon" Margin="536,8,462,664" Background="#FFD6E8FF" Click="Weapon_Click"/>
        <Button x:Name="Artifact" Content="Artifact" Margin="913,8,83,664" Background="#FFD6E8FF" Click="Artifact_Click"/>
    </Grid>
</Window>
