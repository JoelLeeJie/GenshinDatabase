<Window x:Class="GenshinDB_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GenshinDB_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Right" Height="33" Margin="0,0,675,0" TextWrapping="Wrap" Text="GenshinDB" VerticalAlignment="Top" Width="125" FontSize="20" TextAlignment="Center"/>
        <Frame x:Name="PageFrame" Margin="111,0,0,0" NavigationUIVisibility="Hidden"/>
        <Button x:Name="Character" Content="Character" Margin="0,66,707,317" Background="#FFD6E8FF" RenderTransformOrigin="0.5,0.5" Click="Character_Click"/>
        <Button x:Name="Weapon" Content="Weapon" Margin="0,206,707,177" Background="#FFD6E8FF" Click="Weapon_Click"/>
        <Button x:Name="Artifact" Content="Artifact" Margin="0,137,707,246" Background="#FFD6E8FF" Click="Artifact_Click"/>
        <Button x:Name="Config" Content="Config" Margin="0,275,707,108" Background="#FFD6E8FF" Click="Config_Click"/>
        <TextBox x:Name="CharacterNames" HorizontalAlignment="Left" Height="93" Margin="10,331,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="83"/>
    </Grid>
</Window>
